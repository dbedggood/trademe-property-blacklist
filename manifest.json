{
  "manifest_version": 3,
  "name": "TradeMe Property Blacklist",
  "version": "1.0",
  "description": "A Chrome extension to blacklist houses on TradeMe.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
